genrule(
    name = "bin",
    srcs = [
        ":other",
    ],
    out = "bin",
    cmd = """
  echo $SRCS > $OUT
  """,
)

genrule(
    name = "other",
    out = "other",
    cmd = """
  echo foo > $OUT
  """,
)

genrule(
    name = "named_outputs",
    outs = {
        "output1": ["out1"],
        "output2": ["out2"],
    },
    default_outs = [],
    bash = "echo something> $OUT/out1 && echo another>$OUT/out2",
    cmd_exe = "(echo something)> %OUT%\\out1 && (echo another)> %OUT%\\out2",
)

python_binary(
    name = "pybin",
    main_module = "main",
    deps = [
        ":pylib",
    ],
)

python_library(
    name = "pylib",
    srcs = [
        "pylib.py",
    ],
)
